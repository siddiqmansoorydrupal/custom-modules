{
  "name": "square360/menu_join",
  "description": "Joins multiple active menu trails into a single breadcrumb trail.",
  "type": "drupal-custom-module",
  "license": "proprietary",
  "scripts": {
    "code-sniff": [
      "./vendor/bin/phpcs --standard=Drupal --extensions=php,module,inc,install,test,profile,theme,css,info,txt,md --ignore=node_modules,bower_components,vendor .",
      "./vendor/bin/phpcs --standard=DrupalPractice --extensions=php,module,inc,install,test,profile,theme,css,info,txt,md --ignore=node_modules,bower_components,vendor,.out,dist ."
    ]
  },
  "version": "1.0.1",
  "require-dev": {
    "drupal/coder": "^8.3",
    "squizlabs/php_codesniffer": "3.*",
    "dealerdirect/phpcodesniffer-composer-installer": "^0.7.1"
  }
}